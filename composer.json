{
    "name": "drutiny/plugin-distro-common",
    "type": "library",
    "description": "Common Drutiny policies for Drupal distributions",
    "keywords": ["drupal", "audit", "drush", "ssh", "report"],
    "license": "GPL-2.0",
    "authors": [
        {"name": "Sean Hamlin", "email": "iwiifm@gmail.com"},
        {"name": "Josh Waihi", "email": "joshwaihi@gmail.com"}
    ],
    "support": {
        "issues": "https://github.com/drutiny/plugin-distro-common/issues",
        "source": "https://github.com/drutiny/plugin-distro-common"
    },
    "require": {
        "drutiny/drutiny": "2.1.*@dev || 2.2.*@dev || 2.3.x-dev",
        "symfony/yaml": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Drutiny\\Plugin\\DistroCommon\\": "src/",
            "DrutinyTests\\Plugin\\DistroCommon\\": "tests/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "2.*",
        "drupal/coder": "^8.2.0"
    }
}
